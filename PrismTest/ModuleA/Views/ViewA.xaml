<UserControl x:Class="ModuleA.Views.ViewA"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ModuleA.Views"
             mc:Ignorable="d" 
        xmlns:prism="http://prismlibrary.com/"
             d:DesignHeight="300" d:DesignWidth="300" Background="AliceBlue"
          
            
             >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="50"/>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>

        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Text="串口数据测试"  />
        <ComboBox x:Name="comList" ItemsSource="{Binding ComList}" DataContext="{Binding}" Grid.Row="1" Width="100" Height="50">
            
        </ComboBox>
        <Button x:Name="openport" Grid.Row="2" Content="打开串口" Width="100" Height="50"/>
        <TextBlock Grid.Row="3" Height="50" Width="100" Text="{Binding Title}"></TextBlock>
    </Grid>
</UserControl>
